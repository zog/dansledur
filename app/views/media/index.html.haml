= form_tag media_path, method: :get do
  = text_field_tag :search, params[:search]
  = submit_tag :"Get it!", name: nil

%ul.media-mosaic
  - @medias.each do |m|
    %li{class: m.tag_list}
      .title= m.name
      .image
        = link_to image_tag(m.image.url(:thumb)), medium_path(m)

%p= link_to "ADD", new_medium_path